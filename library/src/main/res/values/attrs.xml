<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="Theme">
        <attr name="defaultButtonDrawable" format="reference"/>
        <attr name="accentButtonDrawable" format="reference"/>
        <attr name="flatButtonDrawable" format="reference"/>
        <attr name="accentTextColor" format="reference|color"/>
        <attr name="navigationMenuSeparator" format="reference"/>
        <attr name="listItemBackground" format="reference"/>
        <attr name="settingItemBackground" format="reference"/>
        <attr name="iconColorNormal" format="color|reference"/>
        <attr name="textColorNormal" format="color|reference"/>
        <attr name="cardViewDrawable" format="reference"/>
    </declare-styleable>
    <declare-styleable name="LProgressBar">
        <attr name="max" format="integer" />
        <attr name="progress" format="integer" />
        <attr name="interruptSize" format="dimension" />
        <attr name="showInterrupt" format="boolean" />
        <attr name="backgroundColor" format="color" />
        <attr name="progressColor" format="color" />
        <attr name="interruptColor" format="color" />
    </declare-styleable>
    <declare-styleable name="LTabLayout">
        <attr name="tabOrientation" format="integer" />
    </declare-styleable>
</resources>
